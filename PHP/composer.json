{
  "name": "SyntaxHighlightBot",
  "description": "@SyntaxHighlightBot for Telegram",
  "keywords": [
    "telegram",
    "telegram bot",
    "telegram bot api",
    "telegram sdk",
    "telegram php",
    "syntax",
    "highlight"
  ],
  "type": "library",
  "homepage": "https://github.com/CristianOspina",
  "license": "MIT",
  "authors": [
    {
      "name": "Cristian Ospina",
      "homepage": "https://github.com/CristianOspina"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "irazasyed/telegram-bot-sdk": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8 || ^5.0",
    "phpspec/prophecy": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "Telegram\\Bot\\": "src/"
    },
    "psr-0": {
      "PHPHtmlParser": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Telegram\\Bot\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}